	<xs:complexType name="FB_Feedback">
		<xs:annotation>
			<xs:documentation>Version 1.0  created 23/5/2014 - part of feedback structure
			------------------------------------------------------------
			Version 1.0 updated 23/5/14 Martin O'Reilly and Tarun Nayak
				1 - new structure to support feedback redesign
				
				</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FB_FeedbackCode" type="xs:string"/>
			<xs:element name="FB_FeedbackSeverity" type="xs:string"/>
			<xs:element name="FB_FeedbackText" type="xs:string"/>
			<xs:element name="FB_AdditionalInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For some feedback codes additional information may be required (eg. JCQ uses message level feedback codes 0003-Out of sequence and 0004-Schema validation failed). These will require missing sequence numbers to be provided</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FB_InfoPair" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Eg. for JCQ feedback code 0003, FB_InfoName = "MissingSequenceNumber" and FB_InfoValue="100"</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FB_InfoName" type="xs:string"/>
									<xs:element name="FB_InfoValue" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FB_Record">
		<xs:annotation>
			<xs:documentation>Version 1.1  updated 30/06/2014 - part of feedback structure
			------------------------------------------------------------
			Version 1.0 updated 23/5/14 Martin O'Reilly and Tarun Nayak
				1 - new structure to support feedback redesign
			Version 1.1 updated 30/6/2014 Steve Palmer - as part of JCQ A2C updates - see change log
				</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FB_DataBlockName" type="xs:string"/>
			<xs:element name="FB_EntityName" type="xs:string"/>
			<xs:element name="FB_RecordIDs">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FB_RecordID" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FB_IdName" type="xs:string"/>
									<xs:element name="FB_IdValue" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FB_Feedbacks" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Feedback against the record. </xs:documentation>
